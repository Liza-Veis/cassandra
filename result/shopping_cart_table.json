{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "shopping_cart",
  "type": "object",
  "properties": {
    "basic_info": {
      "type": "object",
      "properties": {
        "birthday": {
          "type": "string"
        },
        "nationality": {
          "type": "string"
        },
        "weight": {
          "type": "string"
        },
        "height": {
          "type": "string"
        }
      }
    },
    "item_count": {
      "type": "number"
    },
    "last_update_timestamp": {
      "type": "string"
    },
    "list": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "my_blob": {
      "type": "string"
    },
    "my_frozen": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "birthday": {
            "type": "string"
          },
          "nationality": {
            "type": "string"
          },
          "weight": {
            "type": "string"
          },
          "height": {
            "type": "string"
          }
        }
      }
    },
    "my_set": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "uniqueItems": true
    },
    "my_tuple": {
      "type": "array",
      "items": [
        {
          "type": "number"
        },
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "teams": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "userid": {
      "type": "string"
    }
  }
}